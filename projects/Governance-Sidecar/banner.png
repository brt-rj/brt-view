<svg width="1200" height="400" xmlns="http://www.w3.org/2000/svg">
  <!-- no-flab-brief: News/Journalism theme with article grid layout -->
  <rect width="1200" height="400" fill="#1A1C18"/>
  
  <!-- Large news article blocks -->
  <rect x="50" y="50" width="350" height="150" fill="none" stroke="#E1AD01" stroke-width="12"/>
  <rect x="50" y="220" width="350" height="130" fill="#E1AD01"/>
  
  <!-- Multiple article columns -->
  <rect x="450" y="50" width="250" height="100" fill="none" stroke="#F4F3EF" stroke-width="10"/>
  <rect x="450" y="170" width="250" height="80" fill="none" stroke="#E1AD01" stroke-width="8"/>
  <rect x="450" y="270" width="250" height="80" fill="#F4F3EF"/>
  
  <!-- Right side article grid -->
  <rect x="750" y="50" width="200" height="130" fill="#E1AD01"/>
  <rect x="750" y="200" width="200" height="150" fill="none" stroke="#F4F3EF" stroke-width="10"/>
  <rect x="975" y="50" width="175" height="300" fill="none" stroke="#E1AD01" stroke-width="12"/>
  
  <!-- Accent lines suggesting text -->
  <line x1="70" y1="90" x2="180" y2="90" stroke="#1A1C18" stroke-width="6"/>
  <line x1="70" y1="110" x2="280" y2="110" stroke="#1A1C18" stroke-width="6"/>
  <line x1="70" y1="130" x2="220" y2="130" stroke="#1A1C18" stroke-width="6"/>
</svg>
